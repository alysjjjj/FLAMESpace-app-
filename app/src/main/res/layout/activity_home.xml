<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9FFF2"
    tools:context=".Home">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="197dp"
        android:layout_height="163dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/h" />

    <ImageView
        android:id="@+id/profile"
        android:layout_width="71dp"
        android:layout_height="66dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_account_circle_24" />

    <Button
        android:id="@+id/button4"
        android:layout_width="139dp"
        android:layout_height="58dp"
        android:layout_marginTop="44dp"
        android:text="Buildings"
        android:backgroundTint="#697B54"
        app:layout_constraintEnd_toStartOf="@+id/button5"
        app:layout_constraintHorizontal_bias="0.753"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <Button
        android:id="@+id/button5"
        android:layout_width="139dp"
        android:layout_height="58dp"
        android:layout_marginEnd="56dp"
        android:backgroundTint="#A6A6A6"
        android:text="Map"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="263dp"
        tools:ignore="MissingConstraints" />


    <androidx.cardview.widget.CardView
        android:layout_width="107dp"
        android:layout_height="130dp"
        android:layout_marginStart="16dp"
        android:backgroundTint="#697B54"
        app:cardCornerRadius="15dp"
        app:layout_constraintEnd_toStartOf="@+id/cardView"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="332dp">

        <ImageView
            android:id="@+id/cite"
            android:layout_width="107dp"
            android:layout_height="90dp"
            android:layout_marginEnd="36dp"
            android:paddingTop="10dp"
            app:srcCompat="@drawable/cite" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="107dp"
        android:layout_height="130dp"
        android:backgroundTint="#697B54"
        app:cardCornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="332dp">

        <ImageView
            android:id="@+id/cma"
            android:layout_width="107dp"
            android:layout_height="90dp"
            android:layout_marginEnd="36dp"
            android:paddingTop="10dp"
            app:srcCompat="@drawable/cma" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="107dp"
        android:layout_height="130dp"
        android:layout_marginEnd="28dp"
        android:backgroundTint="#697B54"
        app:cardCornerRadius="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="332dp">

        <ImageView
            android:id="@+id/cea"
            android:layout_width="107dp"
            android:layout_height="90dp"
            android:layout_marginEnd="36dp"
            android:paddingTop="10dp"
            app:srcCompat="@drawable/cea" />
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>